
create table login(
usernanme varchar(50),
password varchar(50)
);
drop table employees;
CREATE TABLE employees (
	
    name VARCHAR(40),
    fname VARCHAR(40),
    dob VARCHAR(40),
    
    salary VARCHAR(40),
    address VARCHAR(40),
    phone VARCHAR(40),
    email VARCHAR(40),
    education VARCHAR(40),
    designation VARCHAR(40),
    aadhar VARCHAR(40),
    empId varchar(50)
    
   
);
